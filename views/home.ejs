<%- include("./partials/header") %>

<section class="cards-wrapper">
 <% for(var i = 0; i < PostOfProfiles.length ; i++) { %>
      
        <div class="card-grid-space">
          <a class="card" style="--bg-img: url(<%=PostOfProfiles[i].image%>)">
            <div>
              <p><%=PostOfProfiles[i].body%></p>
              <div class="tags">
                <div class="tag"><%=PostOfProfiles[i].tag%> </div>
                <link></bu><i id="like" class="fas fa-thumbs-up"></i></link>
                <i id="comment" class="fas fa-comment-dots"></i>
              </div>
            </div>
          </a>
        </div>



        <!-- https://images.unsplash.com/photo-1520839090488-4a6c211e2f94?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=38951b8650067840307cba514b554ba5&auto=format&fit=crop&w=1350&q=80 -->
      

    <% } %>

</section>

<%- include("./partials/footer") %>




